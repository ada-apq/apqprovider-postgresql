with "apq.gpr";
with "apq-postgresql.gpr";
with "apq_provider.gpr";

project APQ_Provider.postgresql is

	version := $version;
	
	
	
	for Source_Dirs use ( $include_path );
	for Library_Dir use $lib_path;
	for Library_Name use "apq_provider-postgresql";
	for Library_Version use "libapq_provider-postgresql.so." & Version;
	for Library_Kind use APQ_Provider'Library_Kind;
	for Externally_Built use "true";

	
	
	package Compiler is
		for Default_Switches ("ada") use ("-O2", "-gnat05");
	end Compiler;

end APQ_Provider.postgresql;

